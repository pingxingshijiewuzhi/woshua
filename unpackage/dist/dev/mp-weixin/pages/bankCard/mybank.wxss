
page {
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.box {
	width: 100vw;
	height: 100vh;
	overflow: hidden;
	background-color: #FFFFFF;
}
.border {
	width: 94%;
	height: 80rpx;
	background-color: #E5E5E5;
	border-radius: 50rpx 50rpx;
	margin: 30rpx 3% 10rpx 3%;
}
.border_next {
	width: 26rpx;
	height: 36rpx;
	margin-left: 50rpx;
	margin-right: 30rpx;
}
.big_ran {
	width: 100%;
	height: 1080rpx;
	overflow-y: scroll;
}
.ran {
	width: 94%;
	line-height: 100rpx;
	margin: 0 3%;
	border-bottom: solid 1rpx #E5E5E5;
	color: #4F4F4F;
}

