<view class="changeplanes"><view class="mytabbar"><view class="mytabbar_top flexs2"><image class="mytabbar_image" src="../../static/image/0000.png" mode data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><view class="mytabbar_text">换机申请</view><view class=" mytabbar_right"></view></view></view><view class="navtabbar"><view data-event-opts="{{[['tap',[['choose',[0]]]]]}}" class="basestyle" bindtap="__e"><view class="{{[(currentIndex==0)?'active':'']}}">全部</view></view><view data-event-opts="{{[['tap',[['choose',[1]]]]]}}" class="basestyle" bindtap="__e"><view class="{{[(currentIndex==1)?'active':'']}}">等待接收</view></view></view><block wx:for="{{changeplanes}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="nothing"><view class="mytimetitle">{{item.time}}</view><block wx:for="{{item.list}}" wx:for-item="initem" wx:for-index="inindex" wx:key="inindex"><block><view data-event-opts="{{[['tap',[['godetail',['$0'],[[['changeplanes','',index],['list','',inindex,'id']]]]]]]}}" class="recodeitem" bindtap="__e"><view class="recodeitem_left"><view class="left_title addblue">等待回收</view><view class="left_time">{{"申请人:"+initem.form_name}}</view></view><view class="recodeitem_right"><view class="addmycolor"><view class="addfontsize">{{initem.num}}</view>台</view><view class="right_num">{{initem.createtime}}</view></view></view></block></block></view></block></block></view>