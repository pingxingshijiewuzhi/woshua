
page {
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.box {
	width: 100vw;
	height: 100vh;
	overflow: hidden;
	background-color: #FFFFFF;
}
.image_next {
	width: 20rpx;
	height: 30rpx;
}
.border {
	width: 100%;
	line-height: 120rpx;
	border-bottom: solid 1rpx #E5E5E5;
	padding: 0 30rpx;
	font-size: 32rpx;
}
.border_num {
	width: 48rpx;
	line-height: 36rpx;
	border-right: solid 4rpx #6B7FC9;
	color: #6B7FC9;
	margin-right: 18rpx;
}
.big_border {
	width: 100%;
	height: 1160rpx;
	overflow-y: scroll;
}

