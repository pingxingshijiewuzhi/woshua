
.content {
	width: 100vw;
	height: 100vh;
	background-image: url(/static/image/bg_yqhh@2x.png);
	background-size: 100% 100%;
	background-repeat: no-repeat;
}
.kong {
	width: 100%;
	height: 50rpx
}
.top image {
	width: 38rpx;
	height: 34rpx
}
.top {
	width: 65%;
	height: 80rpx;
	color: #FFFFFF;
	font-size: 32rpx;
	font-weight: 700;
	letter-spacing: 4rpx;
}
.yao {
	width: 62%;
	text-align: right;
	line-height: 30px
}
.box {
	width: 78%;
	height: 62%;
	margin: 40rpx auto;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
	background-repeat: no-repeat;
}
.box_t {
	width: 100%;
	height: 30%;
}
.box_t_l {
	width: 50%;
	height: 100%;
}
.box_t image {
	width: 120rpx;
	height: 120rpx;
	border-radius: 60rpx;
}
.name {
	width: 200rpx;
	height: 50rpx;
	line-height: 50rpx;
	border-radius: 25rpx;
	text-align: center;
	margin: 6rpx auto;
	background-color: #587FF8;
	letter-spacing: 4rpx;
}
.box_t_b {
	width: 92%;
	height: 50rpx;
	margin: 10rpx auto;
	letter-spacing: 4rpx;
}
.box_t_b image {
	width: 26rpx;
	height: 30rpx;
}
.box_b {
	width: 100%;
	height: 60%;
	margin-top: 40rpx
}
.box_b image {
	width: 400rpx;
	height: 350rpx;
	margin-top: 40rpx
}
.box_b_code {
	margin: 20rpx 0 10rpx 0;
	letter-spacing: 4rpx;
}
.list {
	width: 80%;
	height: 10%;
	margin: 60rpx auto;
}
.list image {
	width: 120rpx;
	height: 120rpx;
	border-radius: 60rpx;
}

