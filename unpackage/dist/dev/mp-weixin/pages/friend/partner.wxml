<view class="box"><view class="border flexs2"><image class="border_image" src="../../static/image/btn_h_fanhui@2x.png" mode data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><view class="border_text">我推荐的代理</view><view data-event-opts="{{[['tap',[['todengdai',['$event']]]]]}}" class="border_right" bindtap="__e">排行榜</view></view><view class="barner"><view class="barner_input"><input type="text" placeholder="请输入商户姓名" data-event-opts="{{[['confirm',[['seachbtn',['$event']]]],['input',[['__set_model',['','searchvalue','$event',[]]]]]]}}" value="{{searchvalue}}" bindconfirm="__e" bindinput="__e"/></view><view class="flexs2 barner_big"><view class="flexs1"><view class="barner_size">{{data_month}}</view><image class="barner_next" src="../../static/image/icon_h_sanjiao_hui@2x.png" mode data-event-opts="{{[['tap',[['hidden',['$event']]]]]}}" bindtap="__e"></image></view><view class="flexs1 barner_right"><view class="barner_size">{{data_months}}</view><image class="barner_next" src="../../static/image/icon_h_sanjiao_hui@2x.png" mode data-event-opts="{{[['tap',[['hiddens',['$event']]]]]}}" bindtap="__e"></image></view><view class="flexs1"><view class="barner_size">{{data_paoxy}}</view><image class="barner_next" src="../../static/image/icon_h_sanjiao_hui@2x.png" mode data-event-opts="{{[['tap',[['proxyclick',['$event']]]]]}}" bindtap="__e"></image></view><view data-event-opts="{{[['tap',[['chosepaix',['$event']]]]]}}" bindtap="__e"><block wx:if="{{paixu}}"><view class=" "><image class="barner_to" src="../../static/image/icon_sx_shang@2x.png" mode></image><image class="barner_to" src="../../static/image/icon_sx_xia@2x.png" mode></image></view></block><block wx:else><view><image class="barner_to" src="../../static/pic/004.png" mode></image><image class="barner_to" src="../../static/pic/005.png" mode></image></view></block></view></view><view hidden="{{!(tohidden)}}" class="barner_month"><view data-event-opts="{{[['tap',[['choose_btn',['本月']]]]]}}" class="{{['barner_list',(data_month=='本月')?'barnner_active':'']}}" bindtap="__e">本月</view><view data-event-opts="{{[['tap',[['choose_btn',['上月']]]]]}}" class="{{['barner_list',(data_month=='上月')?'barnner_active':'']}}" bindtap="__e">上月</view><view data-event-opts="{{[['tap',[['choose_btn',['累计']]]]]}}" class="{{['barner_list',(data_month=='累计')?'barnner_active':'']}}" bindtap="__e">累计</view></view><view hidden="{{!(tohiddens)}}" class="barner_month"><view data-event-opts="{{[['tap',[['choose_btns',['交易额']]]]]}}" class="{{['barner_list',(data_months=='交易额')?'barnner_active':'']}}" bindtap="__e">交易额</view><view data-event-opts="{{[['tap',[['choose_btns',['商户数']]]]]}}" class="{{['barner_list',(data_months=='商户数')?'barnner_active':'']}}" bindtap="__e">商户数</view></view><view hidden="{{!(toproxystatus)}}" class="barner_month"><view data-event-opts="{{[['tap',[['choose_proxybtn',['代理']]]]]}}" class="{{['barner_list',(data_paoxy=='代理')?'barnner_active':'']}}" bindtap="__e">代理</view><view data-event-opts="{{[['tap',[['choose_proxybtn',['粉丝']]]]]}}" class="{{['barner_list',(data_paoxy=='粉丝')?'barnner_active':'']}}" bindtap="__e">粉丝</view></view></view><view class="list_title">看不到</view><view class="big_list "><block wx:for="{{mydata}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tophotos',['$0'],[[['mydata','',index,'id']]]]]]]}}" class="list flexs2" bindtap="__e"><view class="flexs1"><image class="list_image" src="{{imgUrl+item.avatar}}" mode></image><view><view class="list_name">{{item.nickname}}</view><view class="list_add">{{"本月新增商户："+item.shop_num}}</view></view></view><view class="moneybox"><view class="list_money"><text class="new_money">{{item.money_count}}</text>元</view><view class="list_month">本月商户交易额</view></view></view></block></view></view>